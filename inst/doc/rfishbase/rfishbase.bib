  journal	= {Science},
  keywords	= {Access to Information,Data
		  Collection,Databases,Ecology,Factual,Information
		  Dissemination,Information Management,Information Storage
		  and Retrieval,Information Systems,Internet},
  month		= feb,
  number	= {6018},
  pages		= {703--705},
  pmid		= {21311007},
  title		= {{Challenges and Opportunities of Open Data in Ecology}},
